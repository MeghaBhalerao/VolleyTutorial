<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.volleytutorial">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".json_as_input_param.JsonAsInputParamActivity"></activity>
        <activity android:name=".upload_image_toserver.UploadImageOnServerActivity" />
        <activity android:name=".send_data_toserver.SendDataToServerActivity" />
        <activity android:name=".json_array_request.JsonArrayRequestActivity" />
        <activity android:name=".json_object_request.JsonObjectRequestActivity" />
        <activity android:name=".image_request.ImageRequestActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".string_request.SettingUpRequestQueueActivity" />
        <activity android:name=".string_request.StringRequestActivity" />
    </application>

</manifest>